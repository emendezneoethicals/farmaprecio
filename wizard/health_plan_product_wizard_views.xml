<odoo>
    <!-- Vista del Wizard -->
    <record id="view_import_health_plan_products_wizard" model="ir.ui.view">
        <field name="name">import.health.plan.products.wizard.form</field>
        <field name="model">import.health.plan.products.wizard</field>
        <field name="arch" type="xml">
            <form string="Importar Productos">
                <group>
                    <field name="file" filename="filename"/>
                    <!-- <field name="filename"/> -->
                </group>
                <footer>
                    <button string="Importar" type="object" name="import_products" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


    <!-- AcciÃ³n del Wizard -->
    <record id="action_import_health_plan_products" model="ir.actions.act_window">
        <field name="name">Importar Productos</field>
        <field name="res_model">import.health.plan.products.wizard</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="view_import_health_plan_products_wizard"/>
        <field name="target">new</field>
    </record>
</odoo>
